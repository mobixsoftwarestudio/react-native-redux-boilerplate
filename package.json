{
	"name": "MXRNBoilerplate",
	"version": "0.0.2",
	"private": true,
	"scripts": {
		"android": "react-native run-android",
		"ios": "react-native run-ios",
		"start": "node ./server/deploy.js",
		"test": "jest",
		"watch-mode": "jest --watch --verbose=true",
		"dev-clean": "watchman watch-del-all && rm -rf /tmp/metro-* && react-native start --reset-cache",
		"dev": "react-native start"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "npx prettier --write",
      "eslint --ext .ts,.tsx,.js,.jsx src/"
    ]
  },
	"dependencies": {
		"@react-native-community/masked-view": "^0.1.5",
		"axios": "^0.19.0",
		"babel-preset-react-native": "^4.0.1",
		"immutability-helper": "^3.0.1",
		"json-server": "^0.15.1",
		"lodash": "^4.17.15",
		"react": "16.9.0",
		"react-native": "0.61.5",
		"react-native-dotenv": "^0.2.0",
		"react-native-gesture-handler": "^1.5.3",
		"react-native-reanimated": "^1.4.0",
		"react-native-safe-area-context": "^0.6.2",
		"react-native-screens": "^2.0.0-alpha.22",
		"react-navigation": "^4.0.10",
		"react-navigation-hooks": "^1.1.0",
		"react-navigation-stack": "^2.0.12",
		"react-redux": "^7.1.3",
		"redux": "^4.0.5",
		"redux-actions": "^2.6.5",
		"redux-saga": "^1.1.3",
		"rn-secure-storage": "^2.0.3"
	},
	"devDependencies": {
		"@babel/core": "^7.6.2",
		"@babel/plugin-proposal-class-properties": "^7.7.4",
		"@babel/plugin-transform-runtime": "^7.7.6",
		"@babel/preset-env": "^7.7.7",
		"@babel/preset-typescript": "^7.7.7",
		"@babel/runtime": "^7.6.2",
		"@react-native-community/eslint-config": "^0.0.5",
		"@types/axios": "^0.14.0",
		"@types/jest": "^24.0.25",
		"@types/lodash": "^4.14.149",
		"@types/react": "^16.9.17",
		"@types/react-native": "^0.60.28",
		"@types/react-redux": "^7.1.5",
		"@types/react-test-renderer": "^16.9.1",
		"@types/redux-actions": "^2.6.1",
		"@types/redux-saga": "^0.10.5",
		"@typescript-eslint/eslint-plugin": "^2.15.0",
		"@typescript-eslint/parser": "^2.15.0",
		"babel-jest": "^24.9.0",
		"babel-plugin-module-resolver": "^4.0.0",
		"babel-plugin-root-import": "^6.4.1",
		"eslint": "^6.7.2",
		"eslint-config-airbnb": "^18.0.1",
		"eslint-config-prettier": "^6.9.0",
		"eslint-plugin-import": "^2.19.1",
		"eslint-plugin-jsx-a11y": "^6.2.3",
		"eslint-plugin-prettier": "^3.1.2",
		"eslint-plugin-react": "^7.17.0",
		"husky": "^4.2.1",
		"jest": "^24.9.0",
		"lint-staged": "^10.0.7",
		"metro-react-native-babel-preset": "^0.58.0",
		"prettier": "^1.19.1",
		"react-native-typescript-transformer": "^1.2.13",
		"react-test-renderer": "16.9.0",
		"reactotron-react-native": "^4.0.2",
		"reactotron-redux": "^3.1.2",
		"reactotron-redux-saga": "^4.2.3",
		"supertest": "^4.0.2",
		"ts-jest": "^24.2.0",
		"typescript": "^3.7.4"
	}
}
